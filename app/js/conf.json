{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
     "parser": "evilstreak",
     "dialect": "Maruku"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}